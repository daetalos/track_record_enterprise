{
  "name": "free-nextjs-admin-dashboard",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "quality:check": "npm run lint:check && npm run format:check && npm run type-check",
    "pre-commit:manual": "lint-staged",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ci": "playwright test --reporter=list",
    "test:e2e:summary": "playwright test --reporter=line",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:run && npm run test:e2e:ci",
    "prepare": "husky",
    "docker:build": "docker-compose build",
    "docker:dev": "docker-compose up -d",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down --volumes --remove-orphans",
    "docker:logs": "docker-compose logs -f",
    "validate:pre-docker": "npm run lint:check && npm run type-check && npm run build",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@headlessui/react": "^2.2.4",
    "@prisma/client": "^6.11.1",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.9",
    "@tanstack/react-table": "^8.21.3",
    "@types/js-yaml": "^4.0.9",
    "apexcharts": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "flatpickr": "^4.6.13",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.525.0",
    "next": "15.2.3",
    "next-auth": "^4.24.11",
    "prisma": "^6.11.1",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "swiper": "^11.2.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-transition-group": "^4.4.12",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "prisma-mock": "^0.10.4",
    "tailwindcss": "^4.0.0",
    "tsc-files": "^1.1.4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "node_modules\\.bin\\eslint --fix --max-warnings=0",
      "node_modules\\.bin\\prettier --write"
    ],
    "*.config.{js,ts,mjs}": [
      "node_modules\\.bin\\eslint --fix --max-warnings=0",
      "node_modules\\.bin\\prettier --write"
    ],
    "*.{json,md,css,scss}": [
      "node_modules\\.bin\\prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "node_modules\\.bin\\tsc-files --noEmit"
    ]
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "overrides": {
    "@react-jvectormap/core": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@react-jvectormap/world": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    }
  }
}
